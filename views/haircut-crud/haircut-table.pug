div.card.d-flex
    div.card-header.bg-primary.text-white Haircuts List
    div.card-body 
        div.table-responsive
            table.table.table-bordered
                thead
                    tr
                        th Number
                        th Description
                        th Price
                tbody
                        - 
                            const haircutListData = [{
                                    'Number': 1,
                                    'Description': 'Haircut-',
                                    'Price': 10.00
                                },{
                                    'Number': 2,
                                    'Description': 'Haircut-',
                                    'Price': 15.00
                                }];
                        each haircut in haircutListData
                            tr
                                td= haircut.Number
                                td= haircut.Description + haircut.Number
                                td= '$' + haircut.Price
    div.card-footer
        div.container.d-flex.justify-content-center
            span Discover your style
